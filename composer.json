{
  "name": "vitalik74/yii2-primer",
  "description": "This is Yii2 extension for PrimerCss",
  "version": "0.1.0",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "yii 2",
    "yii2 primer",
    "primerCss",
    "yii2 primer",
    "css framework"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tsibikov Vitaliy",
      "email": "tsibikov_vit@mail.ru",
      "homepage": "http://tsibikov.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/vitalik74/yii2-primer/issues?state=open",
    "source": "https://github.com/vitalik74/yii2-primer",
    "email": "tsibikov_vit@mail.ru"
  },
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "*",
    "bower-asset/primer-css": "@stable",
    "yiisoft/yii2-bootstrap": "*",
    "yiisoft/yii2-swiftmailer": "*",
    "yiisoft/yii2-codeception": "*",
    "codeception/codeception": "2.0.*",
    "codeception/specify": "*",
    "codeception/verify": "*",
    "yiisoft/yii2-faker": "*"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "*",
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-faker": "*"
  },
  "autoload": {
    "psr-4": {
      "vitalik74\\primer\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
